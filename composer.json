{
    "name": "loop54/php-connector",
    "description": "Wrapper around the API for querying your Loop54 engine.",
    "version": "2.2.3",
    "type": "library",
    "keywords": ["search", "service", "api", "ecommerce"],
    "homepage": "https://github.com/LoopFiftyFour/PHP-Connector",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Loop54",
        "homepage": "https://www.loop54.com"
    }, {
        "name": "OpenAPI-Generator contributors",
        "homepage": "https://openapi-generator.tech"
    }],
    "require": {
        "php": "7.3 - 8",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.5|^7.2",
        "ramsey/uuid": "^3.8|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/symfony": "^v3.4",
        "squizlabs/php_codesniffer": "^3.3",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Loop54\\API\\" : "lib/",
            "Loop54\\API\\RemoteClientInfo\\" : "lib/RemoteClientInfo/",
            "Loop54\\API\\OpenAPI\\": "lib/OpenAPI/",
            "Loop54\\API\\OpenAPI\\Model\\": "lib/OpenAPI/Model/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loop54\\API\\": "test/",
            "Loop54\\API\\Examples\\": "examples/",
            "Loop54\\API\\Examples\\App\\": "examples/app/"
        },
        "files": [
            "examples/app/Utils.php"
        ]
    },
    "bin": ["index.php"]
}
